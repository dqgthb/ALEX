    {
      "type": "default",
      "project": "CMakeLists.txt",
      "projectTarget": "benchmark.exe",
      "name": "bm ll ordinary 200M.exe",
      "args": [
        "--keys_file=${workspaceRoot}/resources/longlat-200M.bin.data",
        "--keys_file_type=binary",
        "--init_num_keys=100000000",
